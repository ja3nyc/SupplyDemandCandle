// This namespace holds all indicators and is required. DONT REMOVE
namespace NinjaTrader.Indicator
{
    ///<summary>
    /// The Supply/Demand Candle  (SDC) indicator displays an arrow whenever new supply or demand forms across different time frames. 
    /// </summary>
    [Description(" The Supply/Demand Candle (SDC) indicator displays an arrow whenever new supply or demand forms across different time frames.")]
    [Gui.Design.DisplayName("Supply/Demand Candle")]
    public class SDC : Indicator
    {
        #region Variables

        #endregion
        
        protected override void OnStateChange()
        {
            AddPlot()
        }
    }
}
